#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/phpstudy_pro/WWW/xinkeji/buildadmin/admin/web/node_modules/.pnpm/registry.npmmirror.com+hogan.js@3.0.2/node_modules/hogan.js/bin/node_modules:/mnt/d/phpstudy_pro/WWW/xinkeji/buildadmin/admin/web/node_modules/.pnpm/registry.npmmirror.com+hogan.js@3.0.2/node_modules/hogan.js/node_modules:/mnt/d/phpstudy_pro/WWW/xinkeji/buildadmin/admin/web/node_modules/.pnpm/registry.npmmirror.com+hogan.js@3.0.2/node_modules:/mnt/d/phpstudy_pro/WWW/xinkeji/buildadmin/admin/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/phpstudy_pro/WWW/xinkeji/buildadmin/admin/web/node_modules/.pnpm/registry.npmmirror.com+hogan.js@3.0.2/node_modules/hogan.js/bin/node_modules:/mnt/d/phpstudy_pro/WWW/xinkeji/buildadmin/admin/web/node_modules/.pnpm/registry.npmmirror.com+hogan.js@3.0.2/node_modules/hogan.js/node_modules:/mnt/d/phpstudy_pro/WWW/xinkeji/buildadmin/admin/web/node_modules/.pnpm/registry.npmmirror.com+hogan.js@3.0.2/node_modules:/mnt/d/phpstudy_pro/WWW/xinkeji/buildadmin/admin/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hogan.js/bin/hulk" "$@"
else
  exec node  "$basedir/../hogan.js/bin/hulk" "$@"
fi
